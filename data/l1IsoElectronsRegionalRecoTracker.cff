# Take all pixel tracks but the potential electrons
module hltL1IsoElectronsRegionalCTFFinalFitWithMaterial = FastTrackMerger {

    VInputTag TrackProducers = { 
	globalPixelWithMaterialTracks
    }

#    untracked VInputTag RemoveTrackProducers = { 
#	ctfL1IsoWithMaterialTracks
#    }

    untracked bool SaveTracksOnly = true

}

module hltL1IsoStartUpElectronsRegionalCTFFinalFitWithMaterial = FastTrackMerger {

    VInputTag TrackProducers = { 
	globalPixelWithMaterialTracks
    }

#    untracked VInputTag RemoveTrackProducers = { 
#	ctfL1IsoWithMaterialTracks
#    }

    untracked bool SaveTracksOnly = true

}

# The sequence
sequence HLTL1IsoElectronsRegionalRecoTrackerSequence = {
    globalPixelTracking & 
    hltL1IsoElectronsRegionalCTFFinalFitWithMaterial
}

sequence HLTL1IsoStartUpElectronsRegionalRecoTrackerSequence = {
    globalPixelTracking & 
    hltL1IsoStartUpElectronsRegionalCTFFinalFitWithMaterial
}


