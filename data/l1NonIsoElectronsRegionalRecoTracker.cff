module hltL1NonIsoElectronsRegionalCTFFinalFitWithMaterial = FastTrackMerger {

    VInputTag TrackProducers = { 
	globalPixelGSWithMaterialTracks
    }

#    untracked VInputTag RemoveTrackProducers = { 
#	ctfL1NonIsoWithMaterialTracks
#    }

    untracked bool SaveTracksOnly = true

}

module hltL1NonIsoStartUpElectronsRegionalCTFFinalFitWithMaterial = FastTrackMerger {

    VInputTag TrackProducers = { 
	globalPixelGSWithMaterialTracks
    }

#    untracked VInputTag RemoveTrackProducers = { 
#	ctfL1NonIsoWithMaterialTracks
#    }

    untracked bool SaveTracksOnly = true

}

# The sequence
sequence HLTL1NonIsoElectronsRegionalRecoTrackerSequence = {
    globalPixelGSTracking & 
    hltL1NonIsoElectronsRegionalCTFFinalFitWithMaterial
}

